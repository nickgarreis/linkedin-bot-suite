#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nick/linkedin-bot-suite/node_modules/.pnpm/puppeteer@24.11.2_typescript@5.8.3/node_modules/puppeteer/lib/cjs/puppeteer/node/node_modules:/Users/nick/linkedin-bot-suite/node_modules/.pnpm/puppeteer@24.11.2_typescript@5.8.3/node_modules/puppeteer/lib/cjs/puppeteer/node_modules:/Users/nick/linkedin-bot-suite/node_modules/.pnpm/puppeteer@24.11.2_typescript@5.8.3/node_modules/puppeteer/lib/cjs/node_modules:/Users/nick/linkedin-bot-suite/node_modules/.pnpm/puppeteer@24.11.2_typescript@5.8.3/node_modules/puppeteer/lib/node_modules:/Users/nick/linkedin-bot-suite/node_modules/.pnpm/puppeteer@24.11.2_typescript@5.8.3/node_modules/puppeteer/node_modules:/Users/nick/linkedin-bot-suite/node_modules/.pnpm/puppeteer@24.11.2_typescript@5.8.3/node_modules:/Users/nick/linkedin-bot-suite/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/nick/linkedin-bot-suite/node_modules/.pnpm/puppeteer@24.11.2_typescript@5.8.3/node_modules/puppeteer/lib/cjs/puppeteer/node/node_modules:/Users/nick/linkedin-bot-suite/node_modules/.pnpm/puppeteer@24.11.2_typescript@5.8.3/node_modules/puppeteer/lib/cjs/puppeteer/node_modules:/Users/nick/linkedin-bot-suite/node_modules/.pnpm/puppeteer@24.11.2_typescript@5.8.3/node_modules/puppeteer/lib/cjs/node_modules:/Users/nick/linkedin-bot-suite/node_modules/.pnpm/puppeteer@24.11.2_typescript@5.8.3/node_modules/puppeteer/lib/node_modules:/Users/nick/linkedin-bot-suite/node_modules/.pnpm/puppeteer@24.11.2_typescript@5.8.3/node_modules/puppeteer/node_modules:/Users/nick/linkedin-bot-suite/node_modules/.pnpm/puppeteer@24.11.2_typescript@5.8.3/node_modules:/Users/nick/linkedin-bot-suite/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/puppeteer@24.11.2_typescript@5.8.3/node_modules/puppeteer/lib/cjs/puppeteer/node/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/puppeteer@24.11.2_typescript@5.8.3/node_modules/puppeteer/lib/cjs/puppeteer/node/cli.js" "$@"
fi
